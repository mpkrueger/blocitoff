<h1>Edit List Title</h1>

<div class="row">
  <div class="col-md-4">
    <p>Update your list title here</p>
  </div>
  <div class="col-md-8">
    <%= form_for @list do |f| %>
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title %>
      </div>
      <div class="form-group">
        <%= f.submit "Save", class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>
